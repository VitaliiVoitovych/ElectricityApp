<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!-- Note: For Android please see also Platforms\Android\Resources\values\colors.xml -->

    <Color x:Key="PrimaryDark">#0f1a21</Color>
    <Color x:Key="DarkGreen">#101d24</Color>
    <Color x:Key="MediumDarkGreen">#182b35</Color>
    <Color x:Key="Light">#abb0b3</Color>
    <Color x:Key="LightDarkGreen">#1d3039</Color>

    <SolidColorBrush x:Key="PrimaryDarkBrush" Color="{StaticResource PrimaryDark}"/>
    <SolidColorBrush x:Key="DarkGreenBrush" Color="{StaticResource DarkGreen}"/>
    <SolidColorBrush x:Key="MediumDarkGreenBrush" Color="{StaticResource MediumDarkGreen}"/>
    <SolidColorBrush x:Key="LightBrush" Color="{StaticResource Light}"/>
    <SolidColorBrush x:Key="LightDarkGreenBrush" Color="{StaticResource LightDarkGreen}"/>
</ResourceDictionary>